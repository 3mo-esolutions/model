		"@material/mwc-dialog": "0.25.*",
		"@material/mwc-drawer": "0.25.*",
		"@material/mwc-fab": "0.25.*",
		"@material/mwc-formfield": "0.25.*",
		"@material/mwc-icon": "0.25.*",
		"@material/mwc-icon-button": "0.25.*",
		"@material/mwc-icon-button-toggle": "0.25.*",
