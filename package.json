{
	"name": "@3mo/model",
	"version": "1.0.0",
	"description": "A robust, unified and modern design library to build web applications using native web components. It also includes wrapped Material Web Components by Google's Material Design Language (MDL).",
	"main": "index.ts",
	"author": "3MO",
	"license": "Apache 2.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/3mo-esolutions/model.git"
	},
	"bugs": {
		"url": "https://github.com/3mo-esolutions/model/issues"
	},
	"homepage": "https://github.com/3mo-esolutions/model#readme",
	"scripts": {
		"start": "cd ./samples/sample-client && npm start",
		"dev": "npm run build && concurrently --raw \"npm run build:watch\" \"npm run test:watch\"",
		"lint": "eslint . && lit-analyzer ./packages/**/* --strict --rules.no-missing-import off",
		"build": "webpack",
		"build:watch": "webpack --watch",
		"test": "karma start --single-run",
		"test:watch": "karma start",
		"test:ci": "npm run build && npm run test"
	},
